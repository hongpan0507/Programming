#include <iostream>
using namespace std;

int main() {
	int test = 0x1fffe;
	int test2 = 0;
	cin >> hex >> test2;
	test = test & 1;
	cout << hex << test2 << endl; // prints !!!Hello World!!!
	return 0;
}
