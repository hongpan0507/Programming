#include "Noise.h"

using namespace stk;
using namespace std;

int main(){
	StkFloat output; //creating audio and control signal; floating-point data type
	Noise noise;

	for(unsigned int i = 0; i < 20; i++){
		output = noise.tick();
		cout << "i =" << i << " : output = " << output <<endl;
	}//end of for
}//end of main
